<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="tmkwallet">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>com.boilerplate.entity.Admin</class>
		<class>com.boilerplate.entity.Address</class>
		<class>com.boilerplate.entity.City</class>
		<class>com.boilerplate.entity.Country</class>
		<class>com.boilerplate.entity.Customer</class>
		<class>com.boilerplate.entity.SessionLog</class>
		<class>com.boilerplate.entity.State</class>
		<class>com.boilerplate.entity.User</class>
		<class>com.boilerplate.entity.UserSession</class>
		<class>com.boilerplate.entity.Oauth_access_token</class>
		<class>com.boilerplate.entity.Oauth_code</class>
		<class>com.boilerplate.entity.Oauth_client_token</class>
		<class>com.boilerplate.entity.Oauth_refresh_token</class>
		<class>com.boilerplate.entity.OAuthClient</class>
		<class>com.boilerplate.entity.WebService</class>
		<class>com.boilerplate.entity.RssItem</class>
		
		
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<properties>
			<property name="hibernate.ejb.cfgfile" value="hibernate.cfg.xml" />
			<property name="javax.persistence.lock.timeout" value="12000000" />
		</properties>


	</persistence-unit>
</persistence>
